

<div class="banner">
  <h1>Find a kick ass developer</h1>
  <h3>Search by</h3>
</div>

<div class="container">
  <div class="cards">
      <% @developers.each do |developer| %>
        <p><%= developer.first_name %> <%= developer.last_name %></p>
        <p><%= developer.price_per_hour %></p>
        <p><% developer.programming_languages.each do |l| %>
          <%= l.name %>
        <% end %></p>
        <p><% developer.works.each do |w| %>
          <%= w.photo %>
        <% end %></p>
      <% end %>
  </div>
</div>
